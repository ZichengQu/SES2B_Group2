<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.bean.StudentList" table="theStudentList">
		<id name="studentListId" column="studentListId">
			<generator class="native"></generator>
		</id>
		<property name="workShopId"></property>
		<property name="studentId"></property>
		<property name="isPresent"></property>
		<property name="isEmail"></property>
	</class>
</hibernate-mapping>